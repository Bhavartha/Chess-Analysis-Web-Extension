{
    "manifest_version": 2,
    "name": "Chess Analysis",
    "description": "Analysis on Chess.com is not free. This simple extension opens the game in Lichess Analysis so we can easily analyze our games",
    "version": "1.0",
    "icons": {
        "48": "icon.png",
        "96": "icon@2x.png"
    },
    "content_scripts": [
        {
            "js": [
                "analyze.js"
            ],
            "matches": [
                "*://*.chess.com/*"
            ]
        }
    ],
    "permissions": [
        "*://*.lichess.org/*"
    ]
}